{
  "version": 2,
  "builds": [
    {
      "src": "expense_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "expense_backend/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "expense_backend/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    }
  ]
}